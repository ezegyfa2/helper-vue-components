#carouselIndicators.carousel.slide(data-bs-ride="carousel")
    ol.carousel-indicators
        li(v-for='(carousel_item, index) in carousel_items' :class="{ active: carousel_item.isActive }" :data-target='carousel_item.dataTarget' :data-slide-to='index' :aria-current='carousel_item.isActive' :aria-label='carousel_item.alt')
    .carousel-inner
        .carousel-item(v-for='carousel_item in carousel_items' :class="{ active: carousel_item.isActive }")
            img.d-block.w-100(:src='carousel_item.src' :alt='carousel_item.alt')
    carousel-button(
        v-if='carousel_buttons'
        v-for='carousel_button in carousel_buttons'
        :dataSlideTitle = 'carousel_button.dataSlideTitle'
        :title = 'carousel_button.title'
        :key='carousel_button.title'
    )
